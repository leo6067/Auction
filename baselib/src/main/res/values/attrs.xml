        <attr name="ep_mention_color" format="color" />
        <!--自定义规则的文字的颜色-->
        <attr name="ep_self_color" format="color" />
        <!--链接的图标-->
        <attr name="ep_link_res" format="reference" />
    </declare-styleable>

    <declare-styleable name="MNPasswordEditText">
        <!--密码框的颜色-->
        <attr name="mnPsw_border_color" format="color" />
