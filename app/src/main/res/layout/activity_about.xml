<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.main.mine.activity.AboutActivity">


    <include layout="@layout/layout_custom_title_w" />


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_66"
        android:src="@drawable/login_app" />


    <TextView

        android:id="@+id/version_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dp_66" />


    <TextView
        android:id="@+id/version_info"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_66"
        android:layout_marginTop="@dimen/dp_66"
        android:background="@color/white"
        android:drawableEnd="@drawable/ic_enter_03"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/dp_35"
        android:paddingEnd="@dimen/dp_35"
        android:text="版本更新" />


</LinearLayout>